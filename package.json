    "jquery": "3.4.1",
    "morgan": "~1.9.0",
    "popper.js": "^1.16.1",
    "pug": "2.0.0-beta11",
    "socket.io": "^3.0.5",
    "socket.io-client": "^3.0.5"
  },
  "devDependencies": {
    "@babel/core": "7.1.6",
    "@babel/preset-env": "7.1.6",
    "babel-loader": "8.0.4",
    "eslint": "^7.17.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^4.3.7",
    "lint-staged": "^10.5.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "webpack": "4.26.1",
    "webpack-cli": "3.1.2"
